<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android"
        id="uk.co.safetybank.plugin.sbstaticresources"
        version="0.1.1">

    <name>Safetybank static resources</name>

    <description>Static resources of safetybank mobile app</description>
    <repo>https://github.com/Hless/cordova-plugin-orientation-changer</repo>
    <keywords>safetybank</keywords>
    <license>Commercial</license>

    <author>Safetybank</author>

    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>

    <js-module src="www/sbstaticresources.js" name="sbstaticresources">
        <clobbers target="window.plugins.sbstaticresources" />
    </js-module>
	<asset src="sb/css/bootstrap/bootstrap3.css" target="plugins/sb/css/bootstrap/bootstrap3.css" />

	<asset src="sb/css/fonts/Chalet-LondonNineteenSixty.woff" target="plugins/sb/css/fonts/" />
	<asset src="sb/css/fonts/glyphicons-halflings-regular.woff" target="plugins/sb/css/fonts/" />
	<asset src="sb/css/fonts/asap-regular-webfont.woff" target="plugins/sb/css/fonts/" />

	<asset src="sb/img/logo.svg" target="plugins/sb/img/" />
	<asset src="sb/img/loading.svg" target="plugins/sb/img/svg" />
	<asset src="sb/img/doc.png" target="plugins/sb/img/" />
	<asset src="sb/img/offline.png" target="plugins/sb/img/" />
	<asset src="sb/img/sprite-ico-64@2x.png" target="plugins/sb/img/" />
	<asset src="sb/img/sprite-ico-64.png" target="plugins/sb/img/" />
	<asset src="sb/img/sprite-ico-32@2x.png" target="plugins/sb/img/" />
	<asset src="sb/img/sprite-ico-32.png" target="plugins/sb/img/" />
	<asset src="sb/img/sprite-ico-24@2x.png" target="plugins/sb/img/" />
	<asset src="sb/img/icon/icon.png" target="plugins/sb/img/" />
	<asset src="sb/img/drag-handle.png" target="plugins/sb/img/" />
	<asset src="sb/img/sprite-ico-24.png" target="plugins/sb/img/" />
	<asset src="sb/img/workers.png" target="plugins/sb/img/" />
	<asset src="sb/img/worker.png" target="plugins/sb/img/" />
	<asset src="sb/img/worker-white.png" target="plugins/sb/img/" />
	<asset src="sb/img/profile-white.png" target="plugins/sb/img/" />
		

